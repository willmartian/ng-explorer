{
  "name": "ng-explorer",
  "version": "0.0.2",
  "description": "CLI tool and Claude plugin to search and explore components, services, directives, and other constructs in Angular projects",
  "type": "module",
  "main": "dist/index.js",
  "packageManager": "pnpm@10.28.1",
  "bin": {
    "ng-explorer": "./bin/ng-explorer.js"
  },
  "files": [
    "dist",
    "bin",
    ".claude-plugin",
    "skills"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "angular",
    "cli",
    "compodoc",
    "explorer",
    "documentation",
    "claude-code",
    "claude-plugin"
  ],
  "author": "Will Martin",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willmartian/ng-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/willmartian/ng-explorer/issues"
  },
  "homepage": "https://github.com/willmartian/ng-explorer#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "fuse.js": "^7.0.0",
    "minimatch": "^9.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  }
}
